msgid "<i>Example:</i>"
msgstr "<i>Voorbeeld:</i>"

#: rapid/glade3/prefs.ui.h:76
msgid "<i>/media/externaldrive/Photos</i>"
msgstr "<i>/media/externaldrive/Fotos</i>"

